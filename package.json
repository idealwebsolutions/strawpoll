{
  "name": "strawpoll",
  "version": "1.0.0",
  "description": "Create, manage, and view popular polls",
  "main": "index.js",
  "scripts": {
    "start:dev": "export DEBUG=knex:tx,controller:poll,controller:vote,schema:* NODE_ENV=development && npm run build-all:dev && node index | pino-colada",
    "r": "node index | pino-colada",
    "start": "export NODE_ENV=production && npm run build-all && node index | pino-colada",
    "build-all:dev": "rm -f public/*.js public/*.css && node_modules/webpack/bin/webpack.js --progress --mode=development && npm run inject-css && npm run inject-js",
    "build-all": "rm -f public/*.js public/*.css && node_modules/webpack/bin/webpack.js --progress --config webpack.config.prod.js --mode=production && npm run inject-css && npm run inject-js",
    "inject-js": "sed -i -e '/<!-- inject:js -->/!b;n;c\\  <script defer src=\"/static/'$(basename public/app-*.js)'\"></script>' resources/views/components/page-footer.marko",
    "inject-css": "sed -i -e '/<!-- inject:css -->/!b;n;c\\  <link rel=\"stylesheet\" media=\"all\" href=\"/static/'$(basename public/app-*.css)'\">' resources/views/components/page-header.marko",
    "reset-db:dev": "node_modules/knex/bin/cli.js migrate:rollback && node_modules/knex/bin/cli.js migrate:latest",
    "reset-db": "node_modules/knex/bin/cli.js migrate:rollback && node_modules/knex/bin/cli.js migrate:latest --env production",
    "test": "standard"
  },
  "author": "idealwebsolutions",
  "license": "MIT",
